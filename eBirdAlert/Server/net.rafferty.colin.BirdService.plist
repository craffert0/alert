<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>net.rafferty.colin.BirdService</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/BirdService</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
